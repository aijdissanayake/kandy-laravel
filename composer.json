{
    "name": "kandy-io/kandy-laravel",
    "description": "This is a Laravel package that encapsulate all Kandy JS SDK and Kandy Restful APIs. Kandy is a product by Gendband which utilizes WebRTC to enable peer to peer video, audio, chat, SMS and PSTN.",
    "keywords": [
        "kandy",
        "streaming",
        "webrtc",
        "video",
        "audio",
        "chat",
        "streaming",
        "video streaming",
        "laravel webrtc"
    ],
    "homepage": "https://github.com/Kandy-IO/kandy-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Kodeplus",
            "email": "kodeplusdev@gmail.com",
            "homepage": "http://kandy.io"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/Models",
            "src/Kodeplusdev/Kandylaravel/Console"
        ],
        "psr-0": {
            "Kodeplusdev\\Kandylaravel\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
