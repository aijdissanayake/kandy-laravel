{
    "name": "kodeplusdev/kandylaravel",
    "description": "Kandy Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Kandy Laravel",
            "email": "kodeplusdev@gmail.com",
            "homepage": "http://kodeplus.net"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/Models",
            "src/Kodeplusdev/Kandylaravel/Console"
        ],
        "psr-0": {
            "Kodeplusdev\\Kandylaravel\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan asset:publish kodeplusdev/kandylaravel"
        ],
        "post-update-cmd": [
            "php artisan asset:publish kodeplusdev/kandylaravel"
        ]
    },
    "minimum-stability": "stable"
}
